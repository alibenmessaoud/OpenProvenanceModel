<xs:schema targetNamespace="http://openprovenance.org/model/extension/v1.01.a"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:opm="http://openprovenance.org/model/v1.01.a"
           xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           jxb:version="2.0">

  <xs:import namespace="http://openprovenance.org/model/v1.01.a"
		     schemaLocation="./opm.1_01.xsd" />



  <xs:complexType name="NamedWasDerivedFrom">
	<xs:complexContent>
	  <xs:extension base="opm:WasDerivedFrom">
		<xs:sequence>
		  <xs:element name="type" type="xs:string"/>
		</xs:sequence>
	  </xs:extension>
	</xs:complexContent>
  </xs:complexType>
  


</xs:schema>
