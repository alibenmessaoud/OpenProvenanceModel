<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://openprovenance.org/model/opmPrinterConfig">

  <graphName>OPMGraph</graphName>


  <processes displayValue="true" coloredAsAccount="true" fontSize="18">
    <process value="http://process.org/add1ToAll" display="add1ToAll"/>
    <process value="http://process.org/accessor"     display="accessor"/>
    <process value="http://process.org/plus1"     display="+1"/>
    <process value="http://process.org/constructor" display="constructor"/>
    <process value="http://process.org/fry"       display="fry"/>
    <process value="http://process.org/bake"      display="bake"/>
    <process value="http://process.org/badBake"   display="badBake"/>
  </processes>

  <artifacts displayValue="true" coloredAsAccount="true" fontSize="18">
  </artifacts>

  <agents displayValue="true" coloredAsAccount="true" fontSize="18">
  </agents>



  <edges default="filled">
    <edge type="org.openprovenance.model.Used"            style="dashed" penwidth="2"/>
    <edge type="org.openprovenance.model.WasGeneratedBy"  style="dashed" penwidth="2"/>
    <edge type="org.openprovenance.model.WasControlledBy" style="dotted" penwidth="2"/>
    <edge type="org.openprovenance.model.WasTriggeredBy"  style="dashed"/>
    <edge type="org.openprovenance.model.WasDerivedFrom"  style="bold"/>
  </edges>


  <accounts defaultAccount="black" defaultColor="black">
    <account name="dataLevel"          color="black"/>
    <account name="processLevel"       color="red"/>
    <account name="detailedLevel"      color="blue"/>
  </accounts>

  <annotations defaultColor="gray" defaultFontColor="black">
  </annotations>


</configuration>  
