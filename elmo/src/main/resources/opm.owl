<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xmlns="http://openprovenance.org/ontology#" xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#" xmlns:swrlb="http://www.w3.org/2003/11/swrlb#" xmlns:swrl="http://www.w3.org/2003/11/swrl#" xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:opm="http://openprovenance.org/ontology#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:owl="http://www.w3.org/2002/07/owl#" xml:base="http://openprovenance.org/ontology">

  <!-- Ontology designers/contributors:  Luc Moreau, Simon Miles, Paolo Missier, Paul Groth, Joe Futrelle, Li Ding,  Daniel Garijo Verdejo -->

  <owl:Ontology rdf:about=""/>

  <!-- OPM Root Concept -->

  <owl:Class rdf:ID="Entity"/>

  <!-- OPM Nodes -->

  <owl:Class rdf:ID="Node">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:disjointWith rdf:resource="#Edge"/>
  </owl:Class>
  
  <owl:Class rdf:ID="Process">
    <rdfs:subClassOf rdf:resource="#Node"/>
  </owl:Class>
  
  <owl:Class rdf:ID="Agent">
    <rdfs:subClassOf rdf:resource="#Node"/>
    <owl:disjointWith rdf:resource="#Artifact"/>
    <owl:disjointWith rdf:resource="#Process"/>

  </owl:Class>
  
  <owl:Class rdf:ID="Artifact">
    <rdfs:subClassOf rdf:resource="#Node"/>
    <owl:disjointWith rdf:resource="#Process"/>
  </owl:Class>

  <!-- OPM Edges -->

  <owl:Class rdf:ID="Edge">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>

  <owl:ObjectProperty rdf:ID="cause">
    <rdf:type     rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain  rdf:resource="#Edge"/>
    <rdfs:range   rdf:resource="#Node"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the cause of an Edge</rdfs:comment>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="#causeWasDerivedFrom"/>
    </owl:propertyChainAxiom>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="#causeWasGeneratedBy"/>
    </owl:propertyChainAxiom>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="#causeUsed"/>
    </owl:propertyChainAxiom>

  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="effect">
    <rdf:type     rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain  rdf:resource="#Edge"/>
    <rdfs:range   rdf:resource="#Node"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the effect of an Edge</rdfs:comment>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="#effectWasGeneratedBy"/>
    </owl:propertyChainAxiom>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="#effectWasDerivedFrom"/>
    </owl:propertyChainAxiom>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="#effectUsed"/>
    </owl:propertyChainAxiom>
  </owl:ObjectProperty>

    <!-- http://openprovenance.org/ontology#effectUsed -->

    <owl:ObjectProperty rdf:about="#effectUsed">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Process"/>
        <rdfs:domain rdf:resource="#Used"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#effectWasTriggeredBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Process"/>
        <rdfs:domain rdf:resource="#WasTriggeredBy"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#effectUsed-1 -->

    <owl:ObjectProperty  rdf:ID="effectUsed-1">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Process"/>
        <rdfs:range rdf:resource="#Used"/>
        <owl:inverseOf rdf:resource="#effectUsed"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty  rdf:ID="effectWasTriggeredBy-1">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Process"/>
        <rdfs:range rdf:resource="#WasTriggeredBy"/>
        <owl:inverseOf rdf:resource="#effectWasTriggeredBy"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#effectWasDerivedFrom -->

    <owl:ObjectProperty rdf:ID="effectWasDerivedFrom">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Artifact"/>
        <rdfs:domain rdf:resource="#WasDerivedFrom"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#effectWasDerivedFrom-1 -->

    <owl:ObjectProperty rdf:ID="effectWasDerivedFrom-1">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#WasDerivedFrom"/>
        <owl:inverseOf rdf:resource="#effectWasDerivedFrom"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#effectWasGeneratedBy -->

    <owl:ObjectProperty rdf:ID="effectWasGeneratedBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Artifact"/>
        <rdfs:domain rdf:resource="#WasGeneratedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#effectWasGeneratedBy-1 -->

    <owl:ObjectProperty rdf:ID="effectWasGeneratedBy-1">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#WasGeneratedBy"/>
        <owl:inverseOf rdf:resource="#effectWasGeneratedBy"/>
    </owl:ObjectProperty>
    

    <owl:ObjectProperty rdf:ID="effectWasControlledBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Process"/>
        <rdfs:domain rdf:resource="#WasControlledBy"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:ID="effectWasControlledBy-1">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="#WasControlledBy"/>
        <rdfs:domain rdf:resource="#Process"/>
        <owl:inverseOf rdf:resource="#effectWasControlledBy"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#causeUsed -->

    <owl:ObjectProperty rdf:ID="causeUsed">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Artifact"/>
        <rdfs:domain rdf:resource="#Used"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:ID="causeWasTriggeredBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Process"/>
        <rdfs:domain rdf:resource="#WasTriggeredBy"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#causeWasDerivedFrom -->

    <owl:ObjectProperty rdf:ID="causeWasDerivedFrom">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Artifact"/>
        <rdfs:domain rdf:resource="#WasDerivedFrom"/>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#causeWasGeneratedBy -->

    <owl:ObjectProperty rdf:ID="causeWasGeneratedBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Process"/>
        <rdfs:domain rdf:resource="#WasGeneratedBy"/>
    </owl:ObjectProperty>


    <owl:ObjectProperty rdf:ID="causeWasControlledBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Agent"/>
        <rdfs:domain rdf:resource="#WasControlledBy"/>
    </owl:ObjectProperty>
    


 
    <!-- http://openprovenance.org/ontology#_used -->

    <owl:ObjectProperty rdf:ID="_used">
        <rdfs:range rdf:resource="#Artifact"/>
        <rdfs:domain rdf:resource="#Process"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#effectUsed-1"/>
            <rdf:Description rdf:about="#causeUsed"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    

    <owl:ObjectProperty rdf:ID="_wasTriggeredBy">
        <rdfs:range rdf:resource="#Process"/>
        <rdfs:domain rdf:resource="#Process"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#effectWasTriggeredBy-1"/>
            <rdf:Description rdf:about="#causeWasTriggeredBy"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>

    <!-- http://openprovenance.org/ontology#_used_star -->

    <owl:ObjectProperty rdf:ID="_used_star">
        <rdfs:range rdf:resource="#Artifact"/>
        <rdfs:domain rdf:resource="#Process"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#_used"/>
            <rdf:Description rdf:about="#_wasDerivedFrom_star"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#_used"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#_wasDerivedFrom -->

    <owl:ObjectProperty rdf:ID="_wasDerivedFrom">
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#Artifact"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#effectWasDerivedFrom-1"/>
            <rdf:Description rdf:about="#causeWasDerivedFrom"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#_wasDerivedFrom_star -->

    <owl:ObjectProperty rdf:ID="_wasDerivedFrom_star">
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#Artifact"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#_wasDerivedFrom"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#_wasDerivedFrom"/>
            <rdf:Description rdf:about="#_wasDerivedFrom_star"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#_wasGeneratedBy -->

    <owl:ObjectProperty rdf:ID="_wasGeneratedBy">
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#Process"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#effectWasGeneratedBy-1"/>
            <rdf:Description rdf:about="#causeWasGeneratedBy"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://openprovenance.org/ontology#_wasGeneratedBy_star -->

    <owl:ObjectProperty rdf:ID="_wasGeneratedBy_star">
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#Process"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#_wasDerivedFrom_star"/>
            <rdf:Description rdf:about="#_wasGeneratedBy"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#_wasGeneratedBy"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:ID="_wasControlledBy">
        <rdfs:domain rdf:resource="#Process"/>
        <rdfs:range rdf:resource="#Agent"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#effectWasControlledBy-1"/>
            <rdf:Description rdf:about="#causeWasControlledBy"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    

  <owl:Class rdf:ID="Used">
    <rdfs:subClassOf rdf:resource="#Edge"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:allValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:someValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:allValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:someValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#role"/>
        <owl:someValuesFrom rdf:resource="#Role"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <owl:disjointWith rdf:resource="#WasControlledBy"/>
    <owl:disjointWith rdf:resource="#WasDerivedFrom"/>
    <owl:disjointWith rdf:resource="#WasGeneratedBy"/>
    <owl:disjointWith rdf:resource="#WasTriggeredBy"/>

  </owl:Class>
  
  <owl:Class rdf:ID="WasGeneratedBy">
    <rdfs:subClassOf rdf:resource="#Edge"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:allValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:someValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>


    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:allValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:someValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#role"/>
        <owl:someValuesFrom rdf:resource="#Role"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <owl:disjointWith rdf:resource="#WasTriggeredBy"/>
  </owl:Class>

  <owl:Class rdf:ID="WasControlledBy">
    <rdfs:subClassOf rdf:resource="#Edge"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:allValuesFrom rdf:resource="#Agent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:someValuesFrom rdf:resource="#Agent"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:allValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:someValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#role"/>
        <owl:someValuesFrom rdf:resource="#Role"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <owl:disjointWith rdf:resource="#WasDerivedFrom"/>
    <owl:disjointWith rdf:resource="#WasGeneratedBy"/>
    <owl:disjointWith rdf:resource="#WasTriggeredBy"/>

  </owl:Class>


  <owl:Class rdf:ID="WasTriggeredBy">
    <rdfs:subClassOf rdf:resource="#Edge"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:allValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:someValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:allValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:someValuesFrom rdf:resource="#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WasDerivedFrom">
    <rdfs:subClassOf rdf:resource="#Edge"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:allValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#cause"/>
        <owl:someValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:allValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#effect"/>
        <owl:someValuesFrom rdf:resource="#Artifact"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <owl:disjointWith rdf:resource="#WasGeneratedBy"/>
    <owl:disjointWith rdf:resource="#WasTriggeredBy"/>

  </owl:Class>



  <!-- Accounts -->

  <owl:Class rdf:ID="Account">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>
  
  <owl:ObjectProperty rdf:ID="account">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Edge"/>
          <owl:Class rdf:about="#Node"/>
          <owl:Class rdf:about="#Annotation"/>
          <!-- add here other classes that have accounts -->
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Account"/>
  </owl:ObjectProperty>
  
  <!-- Roles -->
  
  <owl:Class rdf:ID="Role">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#value" />
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:ObjectProperty rdf:ID="role">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Used"/>
          <owl:Class rdf:about="#WasGeneratedBy"/>
          <owl:Class rdf:about="#WasControlledBy"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Role"/>
  </owl:ObjectProperty>
  
  <!-- OPM Graph -->
  
  <owl:Class rdf:ID="OPMGraph">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>
  
  <!-- note the difference of meaning for OPMGraphs: this relation
       gives the accounts known by this graph, as opposed to denote account
       membership. -->
  
  <owl:ObjectProperty rdf:ID="hasAccount">
    <rdfs:domain>
      <owl:Class rdf:about="#OPMGraph"/>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Account"/>
  </owl:ObjectProperty>
  
  
  <owl:ObjectProperty rdf:ID="hasDependency">
    <rdfs:domain rdf:resource="#OPMGraph"/>
    <rdfs:range rdf:resource="#Edge"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="hasArtifact">
    <rdfs:domain rdf:resource="#OPMGraph"/>
    <rdfs:range rdf:resource="#Artifact"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="hasAgent">
    <rdfs:domain rdf:resource="#OPMGraph"/>
    <rdfs:range rdf:resource="#Agent"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasProcess">
    <rdfs:domain rdf:resource="#OPMGraph"/>
    <rdfs:range rdf:resource="#Process"/>
  </owl:ObjectProperty>
  
  
  <!-- OPM Annotation -->
  
  <owl:Class rdf:ID="Annotation">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>
  
  <owl:Class rdf:ID="Property">
    <rdfs:subClassOf rdf:resource="#Entity"/>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#value"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#value"/>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
      </owl:Restriction>
    </rdfs:subClassOf>


    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#uri"/>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:DatatypeProperty rdf:ID="value">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Role"/>
          <owl:Class rdf:about="#Property"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the value of a role or a property</rdfs:comment>
  </owl:DatatypeProperty>

 
<!-- Annotations -->

  <owl:Class rdf:ID="Annotable">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Edge"/>
      <owl:Class rdf:about="#Node"/>
      <owl:Class rdf:about="#Annotation"/>
      <owl:Class rdf:about="#Role"/>
      <owl:Class rdf:about="#OPMGraph"/>
      <owl:Class rdf:about="#Account"/>
      <!-- add here other classes that can be annotated -->
    </owl:unionOf>
  </owl:Class>


  <owl:ObjectProperty rdf:ID="property">
    <rdfs:domain rdf:resource="#Annotation"/>
    <rdfs:range rdf:resource="#Property"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:ID="uri">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain rdf:resource="#Property"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:ID="annotation">
    <rdfs:domain rdf:resource="#Annotable"/>
    <rdfs:range rdf:resource="#Annotation"/>
  </owl:ObjectProperty>

<!-- opm-predefined annotations -->

  <owl:DatatypeProperty rdf:ID="label">
    <rdfs:domain rdf:resource="#Annotable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="profile">
    <rdfs:domain rdf:resource="#Annotable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="type">
    <rdfs:domain rdf:resource="#Annotable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="pname">
    <rdfs:domain rdf:resource="#Annotable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:ID="avalue">
    <rdfs:domain rdf:resource="#Artifact"/>
    <rdfs:range rdf:resource="#AValue"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="AValue">
    <rdfs:subClassOf rdf:resource="#Entity"/>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#content"/>
        <owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#encoding"/>
        <owl:someValuesFrom rdf:resource="&xsd;anyURI"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:DatatypeProperty rdf:ID="content">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain rdf:resource="#AValue"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="encoding">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain rdf:resource="#AValue"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </owl:DatatypeProperty>



  <!-- Time -->

  <owl:Class rdf:ID="OTime">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>

  <owl:DatatypeProperty rdf:ID="noLaterThan">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the event occurred no later than this time</rdfs:comment>
    <rdfs:domain rdf:resource="#OTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="noEarlierThan">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the event occurred no earlier than this time</rdfs:comment>
    <rdfs:domain rdf:resource="#OTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="exactlyAt">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The event occurred exactly at this time. This is meant to be an abbreviation for an OTime where noEarlierThan and noLaterThan are identical. exactlyAt should be  disjoint from noLaterThan  U noEarlierThan. </rdfs:comment>
    <rdfs:domain rdf:resource="#OTime"/>
  </owl:DatatypeProperty>

  <owl:Class rdf:ID="EventEdge">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Used"/>
      <owl:Class rdf:about="#WasGeneratedBy"/>
    </owl:unionOf>
  </owl:Class>


  <owl:ObjectProperty rdf:ID="time">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:range rdf:resource="#OTime"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a piece of timing information associated with an event</rdfs:comment>
    <rdfs:domain rdf:resource="#EventEdge"/>
  </owl:ObjectProperty>



  <owl:ObjectProperty rdf:ID="startTime">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:range rdf:resource="#OTime"/>
    <rdfs:domain rdf:resource="#WasControlledBy"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the time at which the agent began controlling the process</rdfs:comment>
  </owl:ObjectProperty>


  <owl:ObjectProperty rdf:ID="endTime">
    <rdf:type    rdf:resource="&owl;FunctionalProperty" />
    <rdfs:range rdf:resource="#OTime"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the time at which the agent end controlling the process</rdfs:comment>
    <rdfs:domain rdf:resource="#WasControlledBy"/>
  </owl:ObjectProperty>



