<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#" xmlns:swrlb="http://www.w3.org/2003/11/swrlb#" xmlns:swrl="http://www.w3.org/2003/11/swrl#" xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:opm="http://www.ipaw.info/2007/opm#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:owl="http://www.w3.org/2002/07/owl#" xml:base="http://www.ipaw.info/2007/opm">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Entity"/>
  <owl:Class rdf:ID="Node">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="#Edge">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>


  <owl:Class rdf:ID="WasControlledBy">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Edge"/>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="Used">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Edge"/>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="WasGeneratedBy">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Edge"/>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="WasTriggeredBy">
    <rdfs:subClassOf rdf:resource="#Edge"/>
  </owl:Class>

  <owl:Class rdf:ID="WasDerivedFrom">
    <rdfs:subClassOf rdf:resource="#Edge"/>
  </owl:Class>


  <owl:Class rdf:ID="Process">
    <rdfs:subClassOf rdf:resource="#Node"/>
  </owl:Class>
  <owl:Class rdf:ID="Agent">
    <rdfs:subClassOf rdf:resource="#Node"/>
  </owl:Class>
  <owl:Class rdf:ID="Artifact">
    <rdfs:subClassOf rdf:resource="#Node"/>
  </owl:Class>
  <owl:Class rdf:ID="Account">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>
  <owl:Class rdf:ID="TimeInterval">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>

  <owl:Class rdf:ID="Role">
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>

  <owl:ObjectProperty rdf:ID="cause">
    <rdfs:domain rdf:resource="#Edge"/>
    <rdfs:range rdf:resource="#Node"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the cause of an Edge</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="value">
    <rdfs:domain rdf:resource="#Role"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the value of a role</rdfs:comment>
  </owl:ObjectProperty>


  <owl:ObjectProperty rdf:ID="effect">
    <rdfs:domain rdf:resource="#Edge"/>
    <rdfs:range rdf:resource="#Node"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the effect of an Edge</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasRole">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Used"/>
          <owl:Class rdf:about="#WasGeneratedBy"/>
          <owl:Class rdf:about="#WasControlledBy"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Role"/>
  </owl:ObjectProperty>

