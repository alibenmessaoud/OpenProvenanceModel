<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://openprovenance.org/model/opmPrinterConfig">

  <graphName>OPMGraph</graphName>


  <processes displayValue="true" coloredAsAccount="true">
    <process value="http://process.org/add1ToAll" display="add1ToAll"/>
    <process value="http://process.org/split"     display="split"/>
    <process value="http://process.org/plus1"     display="+1"/>
    <process value="http://process.org/cons"      display="cons"/>
    <process value="http://process.org/fry"       display="fry"/>
    <process value="http://process.org/bake"      display="bake"/>
    <process value="http://process.org/badBake"   display="badBake"/>
  </processes>

  <artifacts displayValue="true" coloredAsAccount="true">
  </artifacts>

  <agents displayValue="true" coloredAsAccount="true">
  </agents>

  <edges default="filled">
    <edge type="org.openprovenance.model.Used"            style="dotted" printRole="true"/>
    <edge type="org.openprovenance.model.UsedStar"        style="dotted" />
    <edge type="org.openprovenance.model.WasGeneratedBy"  style="dotted" printRole="true"/>
    <edge type="org.openprovenance.model.WasControlledBy" style="dotted" printRole="true"/>
    <edge type="org.openprovenance.model.WasDerivedFrom"  style="bold"/>
  </edges>


  <accounts defaultAccount="black" defaultColor="black">
    <account name="detailedAccount"      color="black"/>
    <account name="summaryAccount"      color="transparent"/>
    <account name="black"       color="transparent"/>

  </accounts>

  <annotations defaultColor="gray">
  </annotations>
</configuration>  
